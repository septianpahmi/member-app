{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
  "version": 2,
  "builds": [
    {
      "src": "src/backend/api/membership.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/backend/api/membership.js"
    }
  ]
}
